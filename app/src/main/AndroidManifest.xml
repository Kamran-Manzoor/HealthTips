<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.salman.healthtips">

    <application
        android:allowBackup="true"
        android:icon="@drawable/healthlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/healthlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".spalashActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Login">
        </activity>
        <activity android:name=".Signup">
        </activity>
        <activity android:name=".Contenview">
        </activity>
        <activity
            android:name=".Articles"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        </activity>
        <activity android:name=".slidercontent">
        </activity>
        <activity android:name=".Aboutus">
        </activity>
        <activity android:name=".Spalash2">
        </activity>
    </application>

</manifest>